{
  "name": "tutur3u",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "prettier": "^2.8.3",
    "turbo": "^1.7.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.26.2"
}
